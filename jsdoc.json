{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"],
    "exclude": [ "node_modules" ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ""
  },
  "plugins": ["jsdoc-vuex-plugin"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/docdash",
    "destination": "./docs/",
    "recurse": true
  }
}
